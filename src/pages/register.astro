---
import ShowToast from "@/react-components/composables/ShowToast";
import RegisterForm from "@/vue-components/forms/RegisterForm.vue";
import MainLayout from "src/layouts/MainLayout.astro";
Astro.response.headers.set("Cache-Control", "public, max-age=300, s-maxage=3600");


---
<MainLayout>
  <div class="flex items-center justify-center min-h-[78vh] 2xl:min-h-[82vh]">
    <RegisterForm client:load >
      <ShowToast client:load text="Register berhasil!" />
    </RegisterForm>
</div>
</MainLayout>